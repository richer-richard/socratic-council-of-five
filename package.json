{
  "name": "socratic-council",
  "version": "1.0.0",
  "description": "A real-time multi-agent group chat where five AI agents debate any topic",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "dev:cli": "pnpm --filter @socratic-council/cli dev",
    "dev:desktop": "pnpm --filter @socratic-council/desktop dev",
    "build": "pnpm -r build",
    "build:cli": "pnpm --filter @socratic-council/cli build",
    "build:desktop": "pnpm --filter @socratic-council/desktop build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "ai",
    "agents",
    "socratic",
    "debate",
    "group-chat",
    "openai",
    "anthropic",
    "gemini",
    "deepseek",
    "kimi"
  ],
  "author": "Socratic Council Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/richer-richard/socratic-council-of-five.git"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@vitest/coverage-v8": "^2.1.5",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.13.0",
    "vitest": "^2.1.5"
  }
}
