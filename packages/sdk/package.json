{
  "name": "@socratic-council/sdk",
  "version": "1.0.0",
  "description": "AI Provider SDK for Socratic Council - handles all API calls",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest run --config ../../vitest.config.ts"
  },
  "dependencies": {
    "@socratic-council/shared": "workspace:*",
    "undici": "^6.19.2"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
